<objectDataList2D>

    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
    <default  name="">
        <visual>
            <texture file="data/textures/pachinko/pachinko_spriteSheet.png"/>
            <mesh genType="sprite_sheet">
                <spriteSheet file="data/objects/2d/mesh/pachinko.xml"/>
            </mesh>
            <shader id="shader_2d_spriteSheet"/>
        </visual>
        <physics world="(game)">
            <fixtureLst>
                <fixture friction="0.2" density="0.2" restitution="0.2"/>
            </fixtureLst>
        </physics>
    </default>
    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
    
