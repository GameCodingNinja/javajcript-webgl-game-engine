<?xml version="1.0"?>
<strategy defaultGroup="(loadingScreen)">
    
    <node name="loadAnim">
        
        <sprite>
            <scriptList>
                <script animate="State_PlayLoadAnim" group="(state)"/>
            </scriptList>
        </sprite>

    </node>
    
    <node name="load_font">
        
        <sprite>
            <font fontName="dejavu_sans_cond_24" fontString="0%">
                <attributes kerning="-1"/>
            </font>
        </sprite>

    </node>

</strategy>
