<?xml version="1.0"?>
<objectDataList2D>

    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
    <default  name="">
        <visual>
            <texture count="0" file="" compressed="false"/>
            <mesh genType="quad">
                <spriteSheet defIndex="" glyphCount="" columns=""/>
                <scaledFrame thicknessWidth="" thicknessHeight="" centerQuad="" frameBottom=""/>
                <file name=""/>
            </mesh>
            <color r="1" g="1" b="1" a="1"/>
            <shader id="shader_2d"/>
        </visual>
        <physics world="(game)">
            <body type="" linearDamping="0" angularDamping="0" fixedRotation="false"/>
            <fixtureLst>
                <fixture shape="" radius="0" chainLoop="false" friction="0.2" density="0.2" restitution="0.2">
                    <!-- <collisionFilter categoryBits="" maskBits="" groupIndex=""/> -->
                    <!-- <vertLst>
                    vert list for polygon shapes - max of 8
                    Box2D polygons are defined using Counter Clockwise Winding (CCW)
                    <vert x="" y=""/>
                    </vertLst> -->
                    <sizeMod top="0" bottom="0" left="0" right="0"/>
                </fixture>
            </fixtureLst>
        </physics>
        <size width="0" height="0"/>
    </default>
    <!-- DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT DEFAULT -->
  
    <objectList>
        
        <object name="tennis_ball_green">
            <visual>
                <texture file="data/textures/levelArt/tennisball100x100.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureLst>
                    <fixture shape="circle" radius="50" density="0.3" restitution="0.8"/>
                </fixtureLst>
            </physics>
        </object>
        
        <object name="tennis_ball_pink">
            <visual>
                <texture file="data/textures/levelArt/tennisballpink100x100.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureLst>
                    <fixture shape="circle" radius="50" density="0.3" restitution="0.8"/>
                </fixtureLst>
            </physics>
        </object>
        
        <object name="frisbee">
            <visual>
                <texture file="data/textures/levelArt/frisbee100x100.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureLst>
                    <fixture shape="circle" radius="50" density="0.3" restitution="0.5"/>
                </fixtureLst>
            </physics>
        </object>
        
        <object name="bone_biscuit">
            <visual>
                <texture file="data/textures/levelArt/bone_biscuit.png"/>
            </visual>
            <physics>
                <body type="dynamic"/>
                <fixtureLst>
                    <fixture shape="polygon" density="0.1" restitution="0.3">
                        <sizeMod top="-3" bottom="-3" left="-3" right="-3"/>
                    </fixture>
                </fixtureLst>
            </physics>
        </object>

        <object name="ui_tennis_ball_green">
            <visual>
                <texture file="data/textures/levelArt/tennisball100x100.png"/>
            </visual>
        </object>
        
        <object name="ui_tennis_ball_pink">
            <visual>
                <texture file="data/textures/levelArt/tennisballpink100x100.png"/>
            </visual>
        </object>
        
        <object name="ui_frisbee">
            <visual>
                <texture file="data/textures/levelArt/frisbee100x100.png"/>
            </visual>
        </object>
        
        <object name="ui_bone_biscuit">
            <visual>
                <texture file="data/textures/levelArt/bone_biscuit.png"/>
            </visual>
        </object>

        <object name="peg">
            <visual>
                <texture count="2" file="data/textures/levelArt/pin_${NUM}.png"/>
            </visual>
            <physics>
                <body type="static"/>
                <fixtureLst>
                    <fixture shape="circle" radius="18"/>
                </fixtureLst>
            </physics>
        </object>
        
        <object name="bar">
            <visual>
                <texture file="data/textures/levelArt/bar.png"/>
            </visual>
            <physics>
                <body type="static"/>
                <fixtureLst>
                    <fixture shape="polygon">
                        <sizeMod top="-4" bottom="-4" left="-4" right="-4"/>
                    </fixture>
                </fixtureLst>
            </physics>
        </object>
        
        <object name="dog_head">
            <visual>
                <texture file="data/textures/levelArt/Dog_head100x100.png"/>
            </visual>
            <physics>
                <body type="kinematic"/>
                <fixtureLst>
                    <fixture shape="polygon" sensor="true">
                        <sizeMod top="-60" bottom="0" left="10" right="10"/>
                    </fixture>
                </fixtureLst>
            </physics>
        </object>
        
        <object name="level_font">
            <visual>
                <mesh genType="font"/>
                <color r="64" g="125" b="192" a="1"/>
            </visual>
        </object>
        
        <object name="meter_frame">
            <visual>
                <texture file="data/textures/levelArt/16x16_3_pixel_frame.png"/>
                <mesh genType="scaled_frame">
                    <scaledFrame thicknessWidth="5" thicknessHeight="5" centerQuad="false"/>
                </mesh>
                <color r="48" g="97" b="153" a="1"/>
            </visual>
            <size width="175" height="55"/>
        </object>

    </objectList>

</objectDataList2D>

